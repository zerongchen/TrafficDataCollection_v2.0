package com.aotain.tdc.constant;

public enum OpType {
	ADD(1), MOD(2), DEL(3), STOP(4), PLAY(5), STOP_OR_PLAY(6), LOGIN(7), LOGOUT(8),OPERATE_ADD(9);
	private int type;

	private OpType(int type) {
		this.setType(type);
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}
}
